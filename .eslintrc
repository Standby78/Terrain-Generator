{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": 0,
        "no-shadow": 0,
        "no-plusplus": 0,
        "max-len": 0,
        "object-curly-newline": 0,
        "import/prefer-default-export": 0,
        "import/no-cycle": 0,
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    }
}
